{
  "name": "onmotion/yii2-telegram",
  "description": "Support chat for site based on Telegram bot",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","module","chat", "telegram", "bot"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Kozhevnikov",
      "email": "onmotion1@gmail.com",
      "homepage": "http://kozhevnikov.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "yiisoft/yii2": ">=2.0.1",
    "longman/telegram-bot": ">=0.50.0"
  },
  "autoload": {
    "psr-4": {
      "onmotion\\telegram\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}